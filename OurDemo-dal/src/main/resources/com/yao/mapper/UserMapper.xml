<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yao.dao.UserDao">
	<!--查询所有 -->
	<select id="findUserByEmail" parameterType="string" resultType="com.yao.model.User">
		select
		id,username,password,email,phone,roleId,regtime,status,isEmailVerify,activeCode
		from userinfo where email = #{email}
	</select>


</mapper>